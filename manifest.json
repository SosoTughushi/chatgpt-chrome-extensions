{
    "manifest_version": 2,
    "name": "ChatGpt Text Splitter",
    "version": "1.0",
    "permissions": ["activeTab", "https://chat.openai.com/*", "storage"],
    "content_scripts": [
      {
        "matches": ["https://chat.openai.com/*"],
        "js": [ "jquery-3.7.0.min.js", "content.js"]
      }
    ],
    "options_page": "options.html"
}
